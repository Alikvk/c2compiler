module mylib_internal;

import stdio as io;

int32 count = 0;

public func void init() {
    io.printf("internal init()\n");
    destroy();
    count++;
}

func void destroy() {
    io.printf("internal destroy()\n");
    count--;
}

public type Handle struct {
    int32 x;
} @(opaque)

public func Handle* create() {
    return nil;
}

public func void modify(Handle* handle) {
    handle.x++;
}

