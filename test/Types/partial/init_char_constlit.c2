// @warnings{no-unused}
module test;

const int32 NUM = 100;
const int32 NUM2 = 200;

func void test1() {
    int8 a = 1;
    int8 b = a + NUM;
    int8 c = a + NUM2;  // @error{constant value 200 out-of-bounds for type 'int8', range [-128, 127]}
}

func void test2() {
    char d = 250;
    char e = 260;   // @error{constant value 260 out-of-bounds for type 'uint8', range [0, 255]}
}

