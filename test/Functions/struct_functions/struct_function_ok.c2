// @warnings{no-unused}
module test;

type Type struct {
    int32 member;
}

func void Type.init(Type* t) {
    t.member = 0;
}

func int32 Type.getMember(const Type* t) {
    return t.member;
}

func void Type.add(Type* t, int32 n) {
    t.member += n;
}

func void myfunc() {
    Type t;
    t.init();
    int32 m = t.getMember();
    t.add(1);

    Type.init(&t);
    Type.add(&t, 2);
    m = Type.getMember(&t);
}

