language = "C"
type = [ "static", "dynamic" ]

[[modules]]
name = "stdio"

[[modules]]
name = "c_errno"

[[modules]]
name = "stdlib"

[[modules]]
name = "string"

[[modules]]
name = "strings"

[[modules]]
name = "unistd"

[[modules]]
name = "regex"

[[modules]]
name = "ctype"

[[modules]]
name = "stdarg"

[[modules]]
name = "sys_socket"

[[modules]]
name = "sys_time"

[[modules]]
name = "time"

[[modules]]
name = "csetjmp"

[[modules]]
name = "csignal"

